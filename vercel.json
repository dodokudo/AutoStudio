{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/threads/generate-operation/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/plans/update/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/threads/schedule/run/route.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/threads/cron/run",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/threads/schedule/run",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/threads/auto-comment/check",
      "schedule": "10 */2 * * *"
    },
    {
      "path": "/api/cron/comments",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/sales/cron/sync",
      "schedule": "0 15 * * *"
    },
    {
      "path": "/api/sales/cron/sync-bank",
      "schedule": "0 16 * * *"
    },
    {
      "path": "/api/cron/line-report/daily",
      "schedule": "0 21 * * *"
    },
    {
      "path": "/api/cron/line-report/weekly",
      "schedule": "0 22 * * 0"
    }
  ]
}